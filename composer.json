{
    "name": "vanio/vanio-form-extra-bundle",
    "description": "Extends Symfony forms to handle value objects",
    "keywords": ["symfony", "form", "value object"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/vaniocz/vanio-form-extra-bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "consistence/coding-standard": "^0.10.1",
        "slevomat/coding-standard": "^1.0.5"
    },
    "autoload": {
        "psr-4": {"Vanio\\VanioFormExtraBundle\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Vanio\\VanioFormExtraBundle\\Tests\\": "tests/"}
    },
    "scripts": {
        "test": "\"vendor/bin/phpunit\"",
        "lint": "\"vendor/bin/phpcs\" --standard=ruleset.xml --extensions=php --encoding=utf-8 -sp src tests",
        "fix": "\"vendor/bin/phpcbf\" --standard=ruleset.xml --extensions=php --encoding=utf-8 --no-patch -sp src tests"
    }
}
